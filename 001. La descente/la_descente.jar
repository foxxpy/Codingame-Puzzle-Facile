import java.util.*;


class Player {

    public static void main(String args[]) {
        Scanner in = new Scanner(System.in);


        // game loop
        while (true) {
            int highest_mountain = -1;
            int highest_height = -1;

            for (int i = 0; i < 8; i++) {
                int mountainH = in.nextInt();

                if(mountainH > highest_height){
                    highest_mountain = i;
                    highest_height = mountainH;
                }
            }


            System.out.println(highest_mountain); // The index of the mountain to fire on.
        }
    }
}